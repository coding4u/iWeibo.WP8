<UserControl x:Class="TencentWeiboSDK.Controls.OAuthLoginBrowser"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone" SizeChanged="UserControl_SizeChanged_1">
    
    <Grid x:Name="LayoutRoot">
        <phone:WebBrowser  Name="webBrowser1"  IsScriptEnabled="True" Navigating="webBrowser1_Navigating" Navigated="webBrowser1_Navigated" LoadCompleted="webBrowser1_LoadCompleted"/>
    </Grid>
</UserControl>
